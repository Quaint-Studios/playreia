		"globals": "^16.2.0",
		"jsdom": "^26.1.0",
		"mdsvex": "0.12.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"super-sitemap": "^1.0.3",
		"svelte": "^5.34.9",
		"svelte-check": "^4.2.2",
		"svelte-lazy": "^1.2.11",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
